<?xml version="1.0" encoding="utf-8"?>
<Specifications xmlns:f="http://www.ni.com/schemas/Semiconductor/Formula.xsd" schemaVersion="1.0" xmlns="http://www.ni.com/schemas/Semiconductor/Specifications.xsd">
  <Section name="f">
    <f:Formula symbol="SCL">
      <f:Definition>1/t.SCL</f:Definition>
      <f:Comment>SCL clock frequency</f:Comment>
    </f:Formula>
  </Section>
  <Section name="t">
    <f:Formula symbol="SCL">
      <f:Definition>t.LOW + t.HIGH</f:Definition>
      <f:Comment>SCL clock period</f:Comment>
    </f:Formula>
    <f:Formula symbol="LOW">
      <f:Definition>1.5 µs</f:Definition>
      <f:Comment>LOW period of the SCL clock</f:Comment>
    </f:Formula>
    <f:Formula symbol="HIGH">
      <f:Definition>1 µs</f:Definition>
      <f:Comment>HIGH period of the SCL clock</f:Comment>
    </f:Formula>
    <f:Formula symbol="BUF">
      <f:Definition>1.3 µs</f:Definition>
      <f:Comment>Bus free time between a STOP and START condition</f:Comment>
    </f:Formula>
    <f:Formula symbol="HD_STA">
      <f:Definition>0.6 µs</f:Definition>
      <f:Comment>START hold time</f:Comment>
    </f:Formula>
    <f:Formula symbol="SU_STA">
      <f:Definition>0.6 µs</f:Definition>
      <f:Comment>START set-up time</f:Comment>
    </f:Formula>
    <f:Formula symbol="HD_DAT">
      <f:Definition>0</f:Definition>
      <f:Comment>DATA hold time</f:Comment>
    </f:Formula>
    <f:Formula symbol="SU_DAT">
      <f:Definition>500 ns</f:Definition>
      <f:Comment>DATA set-up time</f:Comment>
    </f:Formula>
    <f:Formula symbol="SU_STO">
      <f:Definition>0.6 µs</f:Definition>
      <f:Comment>STOP set-up time</f:Comment>
    </f:Formula>
  </Section>
</Specifications>
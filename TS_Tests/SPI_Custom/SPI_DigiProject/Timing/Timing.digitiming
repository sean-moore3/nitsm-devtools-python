<?xml version="1.0" encoding="utf-8"?>
<TimingFile xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" schemaVersion="1.0" xmlns="http://www.ni.com/Semiconductor/Timing">
  <TimingSheet>
    <TimeSets>
      <TimeSet name="Idle">
        <Period>t.SCK</Period>
        <PinEdges>
          <PinEdge pin="AllPins">
            <DriveNonReturn>
              <On>0</On>
              <Data>0</Data>
              <Off>t.SCK</Off>
            </DriveNonReturn>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Timing while the bus is idle</Comment>
          </PinEdge>
        </PinEdges>
      </TimeSet>
      <TimeSet name="Start">
        <Period>t.SCK</Period>
        <PinEdges>
          <PinEdge pin="CS">
            <DriveNonReturn>
              <On>0</On>
              <Data>0</Data>
              <Off>t.SCK</Off>
            </DriveNonReturn>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Chip select line START condition timing</Comment>
          </PinEdge>
          <PinEdge pin="SCK">
            <ReturnToLow>
              <On>0</On>
              <Data>0</Data>
              <Return>t.setup_cs</Return>
              <Off>t.SCK</Off>
            </ReturnToLow>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Clock line START condition timing</Comment>
          </PinEdge>
          <PinEdge pin="DataPins">
            <DriveNonReturn>
              <On>0</On>
              <Data>0</Data>
              <Off>t.SCK</Off>
            </DriveNonReturn>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Data lines START condition timing</Comment>
          </PinEdge>
        </PinEdges>
      </TimeSet>
      <TimeSet name="Busy">
        <Period>t.SCK</Period>
        <PinEdges>
          <PinEdge pin="CS">
            <DriveNonReturn>
              <On>0</On>
              <Data>0</Data>
              <Off>t.SCK</Off>
            </DriveNonReturn>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Chip select line timing while transferring data</Comment>
          </PinEdge>
          <PinEdge pin="SCK">
            <ReturnToLow>
              <On>0</On>
              <Data>t.LOW</Data>
              <Return>t.LOW + t.HIGH</Return>
              <Off>t.SCK</Off>
            </ReturnToLow>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Clock line timing while transferring data</Comment>
          </PinEdge>
          <PinEdge pin="MOSI">
            <DriveNonReturn>
              <On>0</On>
              <Data>t.LOW - t.setup_mosi</Data>
              <Off>t.SCK</Off>
            </DriveNonReturn>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Slave-input line timing while transferring data</Comment>
          </PinEdge>
          <PinEdge pin="MISO">
            <DriveNonReturn>
              <On>0</On>
              <Data>t.LOW - t.delay_miso</Data>
              <Off>t.SCK</Off>
            </DriveNonReturn>
            <CompareStrobe>
              <Strobe>(t.LOW - t.delay_miso) + t.HIGH/2</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Slave-output line timing while transferring data</Comment>
          </PinEdge>
        </PinEdges>
      </TimeSet>
      <TimeSet name="Stop">
        <Period>t.SCK</Period>
        <PinEdges>
          <PinEdge pin="CS">
            <DriveNonReturn>
              <On>0</On>
              <Data>0</Data>
              <Off>t.SCK</Off>
            </DriveNonReturn>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Chip select line STOP condition timing</Comment>
          </PinEdge>
          <PinEdge pin="SCK">
            <DriveNonReturn>
              <On>0</On>
              <Data>t.LOW</Data>
              <Off>t.SCK</Off>
            </DriveNonReturn>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Clock line STOP condition timing</Comment>
          </PinEdge>
          <PinEdge pin="DataPins">
            <DriveNonReturn>
              <On>0</On>
              <Data>0</Data>
              <Off>t.SCK</Off>
            </DriveNonReturn>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>Data lines STOP condition timing</Comment>
          </PinEdge>
        </PinEdges>
      </TimeSet>
      <TimeSet name="SourceStartSetup">
        <Period>3 Âµs</Period>
        <PinEdges>
          <PinEdge pin="AllPins">
            <DriveNonReturn>
              <On>0</On>
              <Data>0</Data>
              <Off>0</Off>
            </DriveNonReturn>
            <CompareStrobe>
              <Strobe>0</Strobe>
            </CompareStrobe>
            <DataSource>Pattern</DataSource>
            <Comment>A setup delay required by the source_start opcode</Comment>
          </PinEdge>
        </PinEdges>
      </TimeSet>
    </TimeSets>
  </TimingSheet>
</TimingFile>